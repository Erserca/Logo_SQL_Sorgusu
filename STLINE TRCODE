CASE 
      when STLINE.TRCODE = 1 then 'Satınalma Faturası'
      when STLINE.TRCODE = 2 then 'Perakende İade Faturası'
      when STLINE.TRCODE = 3 then 'Toptan Satış İade Faturası'
      when STLINE.TRCODE = 4 and STLINE.STFICHEREF =0 then 'Alınan Hizmet Faturası'
      when STLINE.TRCODE = 4 and STLINE.STFICHEREF >0 then 'Konsinye Çıkış İade İrsaliyesi'
      when STLINE.TRCODE = 5 and STLINE.STFICHEREF =0 then 'Alınan Proforma Faturası'
      when STLINE.TRCODE = 5 and STLINE.STFICHEREF >0 then 'Konsinye Giriş İrsaliyesi'
      when STLINE.TRCODE = 6 then 'Satınalma İade Faturası'
      when STLINE.TRCODE = 7 then 'Perakende Satış Faturası'
      when STLINE.TRCODE = 8 then 'Toptan Satış Faturası'
      when STLINE.TRCODE = 9 and STLINE.STFICHEREF =0 then 'Verilen Hizmet Faturası'
      when STLINE.TRCODE = 9 and STLINE.STFICHEREF >0 then 'Konsinye Çıkış İrsaliyesi'
      when STLINE.TRCODE = 10 and STLINE.STFICHEREF =0 then 'Verilen Proforma Faturası'
      when STLINE.TRCODE = 10 and STLINE.STFICHEREF >0 then 'Konsinye Giriş İade İrsaliyesi'
      when STLINE.TRCODE = 11 then 'Fire Fişi'
      when STLINE.TRCODE = 12 then 'Sarf Fişi'
      when STLINE.TRCODE = 13 and STLINE.STFICHEREF =0 then 'Alınan Fiyat Farkı Faturası'
      when STLINE.TRCODE = 13 and STLINE.STFICHEREF >0 then 'Üretimden Giriş Fişi'
      when STLINE.TRCODE = 14 and STLINE.STFICHEREF =0 then 'Verilen Fiyat Farkı Faturası'
      when STLINE.TRCODE = 14 and STLINE.STFICHEREF >0 then 'Devir Fişi'
      when STLINE.TRCODE = 25 then 'Ambar Fişi'
      when STLINE.TRCODE = 50 then 'Sayım Fazlası'
      when STLINE.TRCODE = 51 then 'Sayım Eksiği'
END AS [BELGE TÜRÜ]
